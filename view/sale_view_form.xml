<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page[@id=&quot;carrier&quot;]" position="inside">
        <label name="is_dpd_shipping"/>
        <field name="is_dpd_shipping"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id=&quot;shipments&quot;]" position="after">
        <page string="DPD" id="dpd">
            <label name="dpd_product"/>
            <field name="dpd_product"/>
            <label name="dpd_customs_terms"/>
            <field name="dpd_customs_terms"/>
        </page>
    </xpath>
</data>
